<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <item>
        <shape>
            <stroke
                android:width="4dp"
                android:color="#FF9102" />
            <padding
                android:bottom="2dp"
                android:top="0.5dp"
                android:left="-0.5dp"
                android:right="2dp"/>

            <corners android:radius="35dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#f4f4f4" />
            <corners android:radius="35dp" />
        </shape>
    </item>
</layer-list>